{
  "detection": {
    "order": [
      "querystring",
      "cookie",
      "header"
    ],
    "lookupQuerystring": "lng",
    "lookupCookie": "language",
    "caches": [
      "cookie"
    ]
  },
  "backend": {
    "loadPath": "./locales/{{lng}}/{{ns}}.json"
  },
  "ns": [
    "app",
    "pay",
    "404",
    "500"
  ],
  "whitelist": [
    "en",
    "cy"
  ],
  "fallbackLng": "en"
}
